<view class="fix-padding"><view class="good container"><view class="group"><view class="title">单号</view><text class="title_color">：</text><input class="input" placeholder="请输入" disabled="{{true}}" data-event-opts="{{[['input',[['__set_model',['','order_num','$event',[]]]]]]}}" value="{{order_num}}" bindinput="__e"/></view><view class="group"><view class="title">林班类型</view><text class="title_color">：</text><picker class="picker" value="{{tIndex}}" range="{{typeList}}" range-key="name" data-event-opts="{{[['change',[['changeType',['$event']]]]]}}" bindchange="__e"><block wx:if="{{tIndex==-1}}"><view class="uni-input">请选择</view></block><block wx:else><view class="uni-input">{{typeList[tIndex].name}}</view></block></picker></view><view class="group"><view class="title">林班名称</view><text class="title_color">：</text><picker class="picker" value="{{nIndex}}" range="{{NumList}}" range-key="quartel_name" data-event-opts="{{[['change',[['changeNum',['$event']]]]]}}" bindchange="__e"><block wx:if="{{nIndex==-1}}"><view class="uni-input">请选择</view></block><block wx:else><view class="uni-input">{{NumList[nIndex].quartel_name}}</view></block></picker></view><view class="group"><view class="title">申请金额</view><text class="title_color">：</text><input class="input" placeholder="请输入" data-event-opts="{{[['input',[['__set_model',['','money','$event',[]]]]]]}}" value="{{money}}" bindinput="__e"/></view><view class="group"><view class="title">备注</view><text class="title_color">：</text><input class="input" placeholder="请输入" data-event-opts="{{[['input',[['__set_model',['','remark','$event',[]]]]]]}}" value="{{remark}}" bindinput="__e"/></view></view><view class="container fix-bottom"><button data-event-opts="{{[['tap',[['add',['$event']]]]]}}" class="btn-submit" bindtap="__e">提交</button></view></view>