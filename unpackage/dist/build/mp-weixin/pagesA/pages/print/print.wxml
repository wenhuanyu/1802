<view><view class="pages"><button class="printbtn" style="background:#4B8EFF;" type="primary" loading="{{isScanning}}" data-event-opts="{{[['tap',[['searchBluetooth',['$event']]]]]}}" bindtap="__e">1.开始搜索</button><view class="device_list"><block wx:for="{{devicesList}}" wx:for-item="item" wx:for-index="index"><view class="device_item" data-index="{{index}}" data-event-opts="{{[['tap',[['connectBluetoothSettings',['$event']]]]]}}" bindtap="__e"><view class="deviceId"><view>{{"deviceId:"+item.deviceId}}</view><block wx:if="{{show_id==item.deviceId}}"><text>已连接</text></block></view><view class="name _div">{{"name:"+item.name}}</view></view></block></view><button class="printbtn" style="background:#4B8EFF;" type="primary" loading="{{isLabelSend}}" disabled="{{isLabelSend}}" data-event-opts="{{[['tap',[['labelTest',['$event']]]]]}}" bindtap="__e">2.标签打印</button><canvas canvas-id="edit_area_canvas"></canvas></view></view>